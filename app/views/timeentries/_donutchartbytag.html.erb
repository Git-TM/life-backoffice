<% sql = "Select * from timeentries WHERE durationinhour > 1" %>
<%= records_array = ActiveRecord::Base.connection.execute(sql) %>

<% records_array.each do |element| %>
<%= p element["tag"] %>
<% end %>



<%= pie_chart [
        {
            name: "freelance",
            data: @timeall.where(categorytime_id: Categorytime.where(name: "freelance").first.id).group(:durationinhour).count,
            # stack: "stack 1"
        },
        {
            name: "code",
            data: @timeall.where(categorytime_id: Categorytime.where(name: "code").first.id).group(:durationinhour).count,
            # stack: "stack 2"
        }
    ], stacked: true, max: 10
%>
