<%= bar_chart [
        {
            name: "Code",
            data: Timeentry.where(date: (params[:search][:start_date].to_s..params[:search][:end_date].to_s)).where(name: "Projet code")
            .group_by_day(:date).sum(:difftime),
            stack: "stack 1"
        },
        {
            name: "Apprentissage",
            data: Timeentry.where(date: (params[:search][:start_date].to_s..params[:search][:end_date].to_s)).where(name: "Apprentissage Code")
            .group_by_day(:date).sum(:difftime),
            stack: "stack 1"
        },
        {
            name: "Freelance",
            data: Timeentry.where(date: (params[:search][:start_date].to_s..params[:search][:end_date].to_s)).where(name: "Freelance")
            .group_by_day(:date).sum(:difftime),
            stack: "stack 1"
        },
        {
            name: "Lecture",
            data: Timeentry.where(date: (params[:search][:start_date].to_s..params[:search][:end_date].to_s)).where(name: "Lecture")
            .group_by_day(:date).sum(:difftime),
            stack: "stack 1"
        },
        {
            name: "Admin / Finance",
            data: Timeentry.where(date: (params[:search][:start_date].to_s..params[:search][:end_date].to_s)).where(name: "Admin / Finance")
            .group_by_day(:date).sum(:difftime),
            stack: "stack 1"
        }

    ], stacked: true, max: 10
%>
